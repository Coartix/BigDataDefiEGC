    "import glob\n",
    "from datetime import datetime\n",
    "import pandas as pd\n",
    "\n",
    "fileNames = glob.glob('networks/*')\n",
    "\n",
    "file_dict = {\n",
    "    datetime.strptime(file.split('/')[1].split('.')[0], '%Y-%m-%d'): pd.read_csv(file)\n",
    "    for file in fileNames\n",
    "}\n",
    "\n",
    "compA = pd.concat(file_dict).sort_index()\n",
    "compA"
   ]
